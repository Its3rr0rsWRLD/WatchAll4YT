{
    "name": "Watch All For YouTube",
    "version": "1.0",
    "description": "Adds a button to YouTube channel pages to watch all videos.",
    "manifest_version": 3,
    "permissions": [],
    "host_permissions": [
      "*://*.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://*.youtube.com/channel/*",
          "*://*.youtube.com/@*"
        ],
        "js": [
          "content-script.js"
        ]
      }
    ]
  }
  